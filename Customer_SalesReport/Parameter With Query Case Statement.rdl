    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
select distinct region, 
case when Region='Asia'
then 'Asia Region'
when Region='Europe'
then 'Europe Rrgion'
when Region='North America'
then 'North America Region'
when Region is null 
then 'No Region'
else Region
end as RegionName
from TotalSale</CommandText>
